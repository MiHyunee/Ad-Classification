{
    "manifest_version": 2,
    "name": "EraseAd",
    "description": "블로그의 광고 글을 분류해줘요!",
    "version": "1.0",

    "browser_action": {
        "default_title": "EraseAd",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "<all_urls>",
        "tabs"
    ]
}